<div class="row">
	<div class="col s12">
	<h2>#<%= tag.name %></h2>

	<h4>Tracks associated with #<%= tag.name %></h4>

	<% tag.tracks.forEach(function(t) { %>
		<div class="divider"></div>
		<div class="section">
			<a href="/tracks/<%= t.id %>">
				<%= t.title %>
			</a>		
		</div>
	<% }); %>
	</div>
</div>